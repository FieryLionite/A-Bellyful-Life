function random_fruit(){
	return choose(IApple, IOrange);
}

function random_sweet_food(){
	return choose(ICakeSlice, ISundea, IFries);
}

function random_softdrink(){
	return choose(IOrangeJuice, IGreenTeaFrap, ISoda, IFrappuccino, IEspresso, IMilk);
}

function random_alcohol(){
	return choose(IBeer, ICocktail1, ICocktail2, ICocktail3, ICocktail4);
}

function random_meal(){
	return choose(IHamburger, IHotdog, ICakeSlice, IFries);
}